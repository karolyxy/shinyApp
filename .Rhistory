mv_rat %>%
group_by(., title) %>%
summarise(., mean_rating = mean(rating)) %>%
sample_n(input$points)
)
output$all_ratings <- renderPlotly(
plot_ly(mv_rat, x= ~title, y= ~mean_rating)
)
})
